   "source": [
    "def optimize(x: np.ndarray, y: np.ndarray, a: float, b: float, learning_rate: float = 0.1):\n",
    "    y_hat = a * x + b\n",
    "    errors = y - y_hat\n",
    "    new_a = a - 2 * learning_rate * np.sum(-errors @ x) / len(y)\n",
    "    new_b = b - 2 * learning_rate * np.sum(-errors) / len(y)\n",
    "\n",
    "    return new_a, new_b"
   ]
